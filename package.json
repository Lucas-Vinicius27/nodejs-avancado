{
  "name": "nodejs-avancado",
  "version": "1.0.0",
  "description": "API using Clean Architecture and TDD.",
  "keywords": [
    "clean architecture",
    "clean code",
    "tdd",
    "ddd",
    "solid",
    "desing patterns",
    "node",
    "typescript",
    "jest",
    "typeorm"
  ],
  "author": {
    "name": "Lucas Vinicius",
    "email": "lucas_vfsantos@hotmail.com",
    "url": "https://lucassantos.app/"
  },
  "repository": "git@github.com:Lucas-Vinicius27/nodejs-avancado.git",
  "bugs": {
    "url": "https://github.com/Lucas-Vinicius27/nodejs-avancado/issues",
    "email": "lucas_vfsantos@hotmail.com"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "lint": "eslint **/*.ts",
    "lint:fix": "yarn run lint --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn run test --watch",
    "test:staged": "yarn run test --findRelatedTests",
    "test:coverage": "yarn run test --coverage",
    "test:integration": "yarn run test -c ./jest.integration.config.js",
    "prepare": "husky install",
    "preci": "rm -rf node_modules",
    "ci": "yarn --frozen-lockfile"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.2",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/module-alias": "^2.0.2",
    "@types/node": "^20.5.1",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^14.0.0",
    "pg-mem": "^2.6.13",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "18.x"
  }
}
